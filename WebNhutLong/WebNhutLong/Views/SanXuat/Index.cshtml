@model IEnumerable<WebNhutLong.Models.tbl_OrderTem>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_1.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Tiếp nhận sản xuất</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->


<div class="row">
    <div class="col-lg-12">

        <div class="panel panel-default">

          
            <!-- /.panel-heading -->
            <div class="panel-body">

                <div class="dataTable_wrapper">

                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>
                                    Mã báo giá
                                </th>
                                <th>
                                    Tên khách hàng
                                </th>
                                <th>
                                    Ngày bắt đầu
                                </th>
                                <th>
                                    Ngày kết thúc
                                </th>
                               
                             
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>

                                        @Html.ActionLink(item.code, "Edit", new { id = item.id }, htmlAttributes: new { @class = "btn btn-outline btn-primary btn-xs" })
                                        
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.customer_id)


                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.date_begin)

                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.date_end)

                                    </td>  
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
